<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asdf.management">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_hanium"
        android:label="HS by Kim"
        android:roundIcon="@mipmap/ic_launcher_hanium_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/Login_name"
            android:theme="@style/RepresentTheme"></activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/ActionBar_name"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/RepresentTheme"></activity>

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@menu/menu" />
    </application>

</manifest>